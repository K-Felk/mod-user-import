{
  "id": "${artifactId}-${version}",
  "name": "User import",
  "provides": [
    {
      "id": "user-import",
      "version": "1.0",
      "handlers": [
        {
          "methods": [
            "GET"
          ],
          "pathPattern": "/user-import"
        },
        {
          "methods": [
            "POST"
          ],
          "pathPattern": "/user-import",
          "modulePermissions": [
            "users.collection.get",
            "addresstypes.collection.get",
            "usergroups.collection.get",
            "users.item.get",
            "users.item.post",
            "users.item.put",
            "perms.users.item.post"
          ]
        }
      ]
    }
  ],
  "requires": [
    {
      "id": "permissions",
      "version": "5.0"
    },
    {
      "id": "users",
      "version": "14.0"
    }
  ]
}
